<?xml version="1.0"?>
<robot name="robot">
    
        <!-- BASE_FOOTPRINT LINK -->

    <joint name="base_footprint_joint" type="fixed">
        <parent link="base_link"/>
        <child link="base_footprint"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
    </joint>

    <link name="base_footprint">
    </link>
    
    
    <!-- base_link (corpo principal) -->
    <link name="base_link">
        <visual>
            <origin xyz="0 0 -0.05" rpy="0 0 -1.57"/> 
      	    <geometry>
        	<mesh filename="file://$(find iris_description)/meshes/chassi_1.stl" scale="1 1 1"/>
      	    </geometry>
            <material name="white">
                <color rgba="1 1 1 1"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 -0.05" rpy="0 0 -1.57"/> 
            <geometry>
                <mesh filename="file://$(find iris_description)/meshes/chassi_1.stl" scale="1 1 1"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0.00000000 0.08125000 0.07750000"/>
            <mass value="1.00000000"/>
  	    <inertia ixx="0.02241803" ixy="-0.00000000" ixz="-0.00000000" iyy="0.03523565" iyz="0.00000000" izz="0.05098702"/>
        </inertial>
    </link>

    <!-- roda direita -->
    <link name="right_wheel">
        <visual>
            <geometry>
                <cylinder radius="0.035" length="0.05"/>
            </geometry>
            <material name="grey">
                <color rgba="0.5 0.5 0.5 1"/>
            </material>

        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.035" length="0.05"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.1"/>
            <inertia ixx="5.1458e-5" ixy="0" ixz="0" iyy="5.1458e-5" iyz="0.0" izz="6.125e-5"/>        
        </inertial>
    </link>

    <joint name="right_wheel_joint" type="continuous">
        <axis xyz="0 0.0 1"/>
        <parent link="base_link"/>
        <child link="right_wheel"/>
        <origin xyz="0 -0.25 -0.05" rpy="-1.5708 0.0 0.0"/>
        <joint_properties damping="1.0" friction="1.0" />
    
    <!-- roda esquerda -->
    </joint>
    <link name="left_wheel">
        <visual>
            <geometry>
                <cylinder radius="0.035" length="0.05"/>
            </geometry>
            <material name="grey">
            </material>

        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.035" length="0.05"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.1"/>
            <inertia ixx="5.1458e-5" ixy="0" ixz="0" iyy="5.1458e-5" iyz="0.0" izz="6.125e-5"/>        </inertial>
    </link>

    <joint name="left_wheel_joint" type="continuous">
        <axis xyz="0 0.0 -1"/>
        <parent link="base_link"/>
        <child link="left_wheel"/>
        <origin xyz="0 0.25 -0.05" rpy="1.5708 0.0 0.0"/>
        <joint_properties damping="1.0" friction="1.0" />
    </joint>

    
    
    
    <!-- roda castor (esferica) -->
    <link name="caster_wheel_f_right">
        <visual>
          <geometry>
            <sphere radius="0.035"/>
          </geometry>
          <material name="grey">
                <color rgba="0.5 0.5 0.5 1"/>
          </material>
        </visual>
        <collision>
          <geometry>
            <sphere radius="0.035"/>
          </geometry>
          <surface>
            <friction>
              <ode>
                <mu>0</mu>
                <mu2>0</mu2>
                <slip1>1.0</slip1>
                <slip2>1.0</slip2>
              </ode>
            </friction>
          </surface>
        </collision>
        <inertial>
          <mass value="0.1"/>
          <inertia ixx="49e-6" ixy="0.0" ixz="0.0" iyy="49e-6" iyz="0.0" izz="49e-6"/>
        </inertial>
    </link>
    <joint name="caster_wheel_f_right_joint" type="fixed">
        <parent link="base_link"/>
        <child link="caster_wheel_f_right"/>
        <origin rpy="0 0 0" xyz="-0.18 -0.13 -0.05"/>
    </joint>
    
    
    
    
    <!-- roda castor (esferica) -->
    <link name="caster_wheel_f_left">
        <visual>
          <geometry>
            <sphere radius="0.035"/>
          </geometry>
          <material name="grey">
                <color rgba="0.5 0.5 0.5 1"/>
          </material>
        </visual>
        <collision>
          <geometry>
            <sphere radius="0.035"/>
          </geometry>
          <surface>
            <friction>
              <ode>
                <mu>0</mu>
                <mu2>0</mu2>
                <slip1>1.0</slip1>
                <slip2>1.0</slip2>
              </ode>
            </friction>
          </surface>
        </collision>
        <inertial>
          <mass value="0.1"/>
          <inertia ixx="49e-6" ixy="0.0" ixz="0.0" iyy="49e-6" iyz="0.0" izz="49e-6"/>
        </inertial>
    </link>
    <joint name="caster_wheel_f_left_joint" type="fixed">
        <parent link="base_link"/>
        <child link="caster_wheel_f_left"/>
        <origin rpy="0 0 0" xyz="-0.18 0.13 -0.05"/>
    </joint>
    
    
    <!-- roda castor (esferica) -->
    <link name="caster_wheel_b_right">
        <visual>
          <geometry>
            <sphere radius="0.035"/>
          </geometry>
          <material name="grey">
                <color rgba="0.5 0.5 0.5 1"/>
          </material>
        </visual>
        <collision>
          <geometry>
            <sphere radius="0.035"/>
          </geometry>
          <surface>
            <friction>
              <ode>
                <mu>0</mu>
                <mu2>0</mu2>
                <slip1>1.0</slip1>
                <slip2>1.0</slip2>
              </ode>
            </friction>
          </surface>
        </collision>
        <inertial>
          <mass value="0.1"/>
          <inertia ixx="49e-6" ixy="0.0" ixz="0.0" iyy="49e-6" iyz="0.0" izz="49e-6"/>
        </inertial>
    </link>
    <joint name="caster_wheel_b_right_joint" type="fixed">
        <parent link="base_link"/>
        <child link="caster_wheel_b_right"/>
        <origin rpy="0 0 0" xyz="0.18 -0.13 -0.05"/>
    </joint>
    
    <!-- roda castor (esferica) -->
    <link name="caster_wheel_b_left">
        <visual>
          <geometry>
            <sphere radius="0.035"/>
          </geometry>
          <material name="grey">
                <color rgba="0.5 0.5 0.5 1"/>
          </material>
        </visual>
        <collision>
          <geometry>
            <sphere radius="0.035"/>
          </geometry>
          <surface>
            <friction>
              <ode>
                <mu>0</mu>
                <mu2>0</mu2>
                <slip1>1.0</slip1>
                <slip2>1.0</slip2>
              </ode>
            </friction>
          </surface>
        </collision>
        <inertial>
          <mass value="0.1"/>
          <inertia ixx="49e-6" ixy="0.0" ixz="0.0" iyy="49e-6" iyz="0.0" izz="49e-6"/>
        </inertial>
    </link>
    <joint name="caster_wheel_b_left_joint" type="fixed">
        <parent link="base_link"/>
        <child link="caster_wheel_b_left"/>
        <origin rpy="0 0 0" xyz="0.18 0.13 -0.05"/>
    </joint>

    


    <!-- laser link e junta -->
    <link name="laser_scanner">
        <visual>
            <geometry>
            	<cylinder radius="0.025" length="0.03"/>
            </geometry>
            
            <material name="black">
                <color rgba="0 0 0 1"/>
            </material>
        </visual>
        
        <collision>
            <geometry>
                <cylinder radius="0.025" length="0.03"/>
            </geometry>
        </collision>

        <inertial>
            <mass value="1e-5"/>
            <inertia ixx="3.81e-9" ixy="0" ixz="0" iyy="3.81e-9" iyz="0.0" izz="1.25e-7"/>
        </inertial>
    </link>

    <joint name="laser_scanner_joint" type="fixed">
        <axis xyz="0.0 1 0"/>
        <parent link="base_link"/>
        <child link="laser_scanner"/>
        <origin xyz="0.21 0.0 0.005" rpy="0.0 0.0 0"/>
    </joint>

    <!-- plugin - laser scanner -->
    <gazebo reference="laser_scanner">
        <sensor type="ray" name="laser">
            <pose>0 0 0 0 0 0</pose> <!-- xyz rpy -->
            <visualize>true</visualize>
            <update_rate>20</update_rate>
            <ray>
                <scan>
                    <horizontal>
                        <samples>720</samples>
                        <resolution>1</resolution>
                        <min_angle>-1.928</min_angle>
                        <max_angle>1.928</max_angle>
                    </horizontal>
                </scan>
                <range>
                    <min>0.1</min>
                    <max>5</max>
                    <resolution>0.1</resolution>
                </range>
            </ray>
            <plugin name="lidar" filename="libgazebo_ros_ray_sensor.so">
            <ros>
            	<remapping>~/out:=scan</remapping>
            </ros>
            	<frame_name>laser_scanner</frame_name>
            	<output_type>sensor_msgs/LaserScan</output_type>
            </plugin>
        </sensor>
    </gazebo>

    <!-- plugin - differential drive -->
    <gazebo>
        <plugin name="bot_controller"   filename="libgazebo_ros_diff_drive.so">
            <left_joint>left_wheel_joint</left_joint>
            <right_joint>right_wheel_joint</right_joint>
            <robot_base_frame>base_link</robot_base_frame>
            <wheel_separation>0.5</wheel_separation>
            <wheel_diameter>0.07</wheel_diameter>
         
            
            <!-- Output -->
            <odometry_frame>odom</odometry_frame>
            <publish_odom>true</publish_odom>
            <publish_odom_tf>true</publish_odom_tf>
            <publish_wheel_tf>true</publish_wheel_tf>
        </plugin>
    </gazebo>

    <!-- cores para o Gazebo -->
    <gazebo reference="base_link">
        <material>Gazebo/White</material>
    </gazebo>

    <gazebo reference="laser_scanner">
        <material>Gazebo/Black</material>
    </gazebo>
    
    <gazebo reference="caster_wheel_b_right">
        <material>Gazebo/Grey</material>
    </gazebo>
    
    <gazebo reference="caster_wheel_b_left">
        <material>Gazebo/Grey</material>
    </gazebo>
    
    <gazebo reference="caster_wheel_f_left">
        <material>Gazebo/Grey</material>
    </gazebo>
    
    <gazebo reference="caster_wheel_f_right">
        <material>Gazebo/Grey</material>
    </gazebo>
    
    <gazebo reference="left_wheel">
        <material>Gazebo/Grey</material>
    </gazebo>
    
    <gazebo reference="right_wheel">
        <material>Gazebo/Grey</material>
    </gazebo>
    
    <gazebo reference="caster_wheel_b_right">
    	<mu1 value="0.001"/>
    	<mu2 value="0.001"/>
    </gazebo>
    
    <gazebo reference="caster_wheel_b_left">
    	<mu1 value="0.001"/>
    	<mu2 value="0.001"/>
    </gazebo>
    
    <gazebo reference="caster_wheel_f_right">
    	<mu1 value="0.001"/>
    	<mu2 value="0.001"/>
    </gazebo>
    
    <gazebo reference="caster_wheel_f_left">
    	<mu1 value="0.001"/>
    	<mu2 value="0.001"/>
    </gazebo>
    
</robot> 
